{
  "name": "penny_university_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "bootstrap": "^4.4.0",
    "classnames": "^2.2.6",
    "cross-fetch": "^3.0.4",
    "deepmerge": "^4.2.2",
    "enzyme-to-json": "^3.4.4",
    "humps": "^2.0.1",
    "moment": "^2.24.0",
    "node-sass-chokidar": "^1.4.0",
    "normalizr": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.2.2",
    "react-mde": "^8.0.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "showdown": "^1.9.1"
  },
  "config": {
    "src_css": "./src",
    "dest_css": "./src"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .js,.jsx",
    "lint:fix": "npm lint --fix",
    "lint:scss": "sass-lint -v $npm_package_config_src_css",
    "lint:scss:fix": "sass-lint-auto-fix",
    "compile:css": "node-sass-chokidar --include-path ./src --include-path ../scss --include-path ./node_modules $npm_package_config_src_css -o $npm_package_config_dest_css && cp ./home.css ../../penny_university/static/css/styles.css",
    "watch:css": "npm run compile:css && node-sass-chokidar --include-path ../scss --include-path ./src --include-path ./node_modules $npm_package_config_src_css -o $npm_package_config_dest_css --watch --recursive"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-airbnb": "^18.1.0",
    "fetch-mock": "^9.0.0-beta.2",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.20.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
